[include]
  path = ~/.gitconfig.local
[hub]
  protocol = https
[alias]
  co = checkout
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[core]
  excludesfile = ~/.gitignore
  editor = code --wait
[apply]
  whitespace = nowarn
[mergetool]
  keepBackup = false
[push]
  default = simple
[difftool]
  prompt = false
[help]
  autocorrect = 1
[credential]
	helper = osxkeychain
[pull]
	rebase = false
[init]
	defaultBranch = main


