set nocompatible
syntax on
set number
set autoindent
set smartindent
set ruler
set showcmd
set showmatch
set expandtab
set cursorline
set hlsearch
set incsearch

set backspace=2
set softtabstop=2
set shiftwidth=2
set tabstop=2

filetype indent plugin on
autocmd FileType yaml setlocal ts=2 sts=2 sw=2 expandtab


